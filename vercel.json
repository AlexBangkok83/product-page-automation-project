{
  "buildCommand": null,
  "framework": null,
  "installCommand": null,
  "outputDirectory": ".",
  "functions": {
    "server.js": {
      "maxDuration": 30
    },
    "api/**/*.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/admin/(.*)",
      "destination": "/server.js"
    },
    {
      "source": "/api/(.*)",
      "destination": "/server.js"
    },
    {
      "source": "/(.*)",
      "destination": "/server.js"
    }
  ],
  "headers": [
    {
      "source": "/stores/**/*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=300"
        }
      ]
    },
    {
      "source": "/stores/**/*.{css,js,png,jpg,jpeg,gif,svg,webp,ico}",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "trailingSlash": false
}