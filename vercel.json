{
  "version": 2,
  "rewrites": [
    {
      "source": "/products/(.*)",
      "destination": "/stores/clipia.fi/products/$1.html"
    },
    {
      "source": "/(.*)",
      "destination": "/stores/clipia.fi/$1.html"
    },
    {
      "source": "/",
      "destination": "/stores/clipia.fi/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  }
}